# {{ ansible_managed }}

# EditorConfig is awesome: http://EditorConfig.org

# top-most EditorConfig file
root = true

# all files
[*]
charset = utf-8
end_of_line = lf
indent_size = 4
indent_style = space
insert_final_newline = true
trim_trailing_whitespace = true

[*.{rb,yaml,yml,lua}]
indent_style = space
indent_size = 2

[{package.json,.travis.yml}]
indent_size = 2

[*.{xml,java,go,sh}]
indent_style = tab

[Makefile]
indent_style = tab
